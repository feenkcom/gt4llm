				"code" : "contextBuilder := [ GtLlmContext new\r\t\tinstance: GtLlmWardleyMapCreator new;\r\t\tisImmutableInstance: true;\r\t\tchecker: (GtLlmAlternativeCodeStep new\r\t\t\t\taddAlternative: GtLlmWardleyMapNodeStep new\r\t\t\t\t\tmatching: [ :aContext | aContext lastResult isForLanguage: 'node' ];\r\t\t\t\taddAlternative: GtLlmWardleyMapPositionStep new\r\t\t\t\t\tmatching: [ :aContext | aContext lastResult isForLanguage: 'position' ];\r\t\t\t\taddAlternative: GtLlmWardleyMapConnectionStep new\r\t\t\t\t\tmatching: [ :aContext | aContext lastResult isForLanguage: 'connection' ]) ]"
